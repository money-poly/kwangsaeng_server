{
    "name": "kwangsange-api",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "NODE_ENV=dev nest start --watch",
        "start:stage": "NODE_ENV=stage nest start --watch",
        "start:dev": "NODE_ENV=local nest start --watch",
        "start:debug": "NODE_ENV=local nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "docker:local": "docker-compose -f local-compose.yml --env-file src/global/config/envs/.local.env up",
        "docker:build": "docker-compose -f local-compose.yml --env-file src/global/config/envs/.local.env build",
        "docker:down": "docker-compose -f local-compose.yml --env-file src/global/config/envs/.local.env down",
        "docker:exec": "docker exec -it api /bin/sh"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.484.0",
        "@nestjs/axios": "^3.0.1",
        "@nestjs/cache-manager": "^2.1.1",
        "@nestjs/common": "^10.0.0",
        "@nestjs/config": "^3.1.1",
        "@nestjs/core": "^10.0.0",
        "@nestjs/jwt": "^10.1.1",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "^10.0.0",
        "@nestjs/throttler": "^5.1.1",
        "@nestjs/typeorm": "^10.0.0",
        "@types/multer-s3": "^3.0.3",
        "axios": "^1.6.2",
        "cache-manager": "^5.3.2",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "dynamoose": "^3.2.1",
        "joi": "^17.11.0",
        "multer-s3": "^3.0.1",
        "mysql2": "^3.6.2",
        "nest-winston": "^1.9.4",
        "nestjs-dynamoose": "^0.5.5",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.1",
        "ts-jenum": "^2.2.2",
        "typeorm": "^0.3.17",
        "typeorm-naming-strategies": "^4.1.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^10.0.0",
        "@nestjs/schematics": "^10.0.0",
        "@nestjs/testing": "^10.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.5.0",
        "prettier": "^3.0.0",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.0",
        "ts-loader": "^9.4.3",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.3",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "jest": {
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "roots": [
            "<rootDir>"
        ],
        "modulePaths": [
            "<rootDir>"
        ],
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
